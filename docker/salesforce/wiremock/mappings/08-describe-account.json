{
  "name": "Describe Account Object",
  "request": {
    "method": "GET",
    "urlPathPattern": "/services/data/v[0-9]+\\.[0-9]+/sobjects/Account/describe"
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "name": "Account",
      "label": "Account",
      "fields": [
        {
          "name": "Id",
          "type": "id",
          "label": "Account ID",
          "length": 18
        },
        {
          "name": "Name",
          "type": "string",
          "label": "Account Name",
          "length": 255
        },
        {
          "name": "Type",
          "type": "picklist",
          "label": "Account Type",
          "picklistValues": [
            {"value": "Customer", "label": "Customer"},
            {"value": "Partner", "label": "Partner"},
            {"value": "Prospect", "label": "Prospect"}
          ]
        },
        {
          "name": "Industry",
          "type": "picklist",
          "label": "Industry"
        },
        {
          "name": "AnnualRevenue",
          "type": "currency",
          "label": "Annual Revenue"
        }
      ],
      "createable": true,
      "updateable": true,
      "deletable": true,
      "queryable": true
    }
  }
}
