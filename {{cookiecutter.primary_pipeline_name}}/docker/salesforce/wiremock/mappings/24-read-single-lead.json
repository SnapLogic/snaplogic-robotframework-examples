{
  "priority": 1,
  "name": "Read Single Lead by ID",
  "request": {
    "method": "GET",
    "urlPathPattern": "/services/data/v[0-9]+\\.[0-9]+/sobjects/Lead/[0-9][A-Za-z0-9]+"
  },
  "response": {
    "status": 200,
    "headers": { "Content-Type": "application/json" },
    "body": "{\"attributes\":{\"type\":\"Lead\",\"url\":\"{{request.path}}\"},\"Id\":\"{{request.pathSegments.[5]}}\",\"FirstName\":\"John\",\"LastName\":\"Smith\",\"Company\":\"Acme Inc\",\"Email\":\"john.smith@acme.com\",\"Phone\":\"(555) 345-6789\",\"Status\":\"Open - Not Contacted\",\"LeadSource\":\"Web\"}",
    "transformers": ["response-template"]
  }
}
