{
  "priority": 1,
  "name": "Read Single Account by ID",
  "request": {
    "method": "GET",
    "urlPathPattern": "/services/data/v[0-9]+\\.[0-9]+/sobjects/Account/[0-9][A-Za-z0-9]+"
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "body": "{\"attributes\":{\"type\":\"Account\",\"url\":\"{{request.path}}\"},\"Id\":\"{{request.pathSegments.[5]}}\",\"Name\":\"Mock Account\",\"Type\":\"Customer\",\"Industry\":\"Technology\",\"Phone\":\"(555) 123-4567\",\"Website\":\"www.mockaccount.com\",\"AnnualRevenue\":1000000,\"NumberOfEmployees\":100}",
    "transformers": ["response-template"]
  }
}
