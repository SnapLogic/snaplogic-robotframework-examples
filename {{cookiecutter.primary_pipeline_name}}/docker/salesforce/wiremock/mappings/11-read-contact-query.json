{
  "priority": 1,
  "name": "Query Contacts via SOQL",
  "request": {
    "method": "GET",
    "urlPathPattern": "/services/data/v[0-9]+\\.[0-9]+/query.*",
    "queryParameters": {
      "q": {
        "contains": "Contact"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": { "Content-Type": "application/json" },
    "jsonBody": {
      "totalSize": 1,
      "done": true,
      "records": [
        {
          "attributes": {
            "type": "Contact",
            "url": "/services/data/v59.0/sobjects/Contact/003000000000TEST01"
          },
          "Id": "003000000000TEST01",
          "FirstName": "Jane",
          "LastName": "Doe",
          "Email": "jane.doe@test.com",
          "Phone": "(555) 234-5678",
          "AccountId": "001000000000TEST01",
          "Title": "VP of Sales",
          "Department": "Sales"
        }
      ]
    },
    "transformers": ["response-template"]
  }
}
