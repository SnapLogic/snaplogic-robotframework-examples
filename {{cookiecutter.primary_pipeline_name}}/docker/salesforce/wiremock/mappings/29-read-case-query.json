  "priority": 1,
  "name": "Query Cases via SOQL",
  "request": {
    "method": "GET",
    "urlPathPattern": "/services/data/v[0-9]+\\.[0-9]+/query.*",
    "queryParameters": {
      "q": {
        "contains": "Case"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": { "Content-Type": "application/json" },
    "jsonBody": {
      "totalSize": 1,
      "done": true,
      "records": [
        {
          "attributes": {
            "type": "Case",
            "url": "/services/data/v59.0/sobjects/Case/500000000000TEST01"
          },
          "Id": "500000000000TEST01",
          "Subject": "Login Issue",
          "Description": "Customer cannot log in to the portal",
          "Status": "New",
          "Priority": "High",
          "Origin": "Email",
          "AccountId": "001000000000TEST01",
          "ContactId": "003000000000TEST01"
        }
      ]
    },
    "transformers": ["response-template"]
  }
}
